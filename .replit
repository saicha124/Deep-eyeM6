modules = ["web", "bash", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["cacert", "cargo", "file", "freetype", "geckodriver", "glibcLocales", "libiconv", "libxcrypt", "libyaml", "openssl", "pkg-config", "rustc", "xcodebuild", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Deep Eye Scanner"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Deep Eye Web GUI"

[[workflows.workflow]]
name = "Deep Eye Scanner"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python deep_eye.py --help"

[[workflows.workflow]]
name = "Deep Eye Web GUI"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python web_gui.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8888
externalPort = 80

[[ports]]
localPort = 44551
externalPort = 3000
